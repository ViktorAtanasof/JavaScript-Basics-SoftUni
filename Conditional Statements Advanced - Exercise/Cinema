function cinema(input) {
    let ticket = input[0];
    let r = Number(input[1]);
    let c = Number(input[2]);
    let stonks = 0;

    if (ticket === "Premiere") {
        stonks = r * c * 12.00;
    } else if(ticket === "Normal") {
        stonks = r * c * 7.50;
    } else if(ticket === "Discount") {
        stonks = r * c * 5.00;
    }
    console.log(`${stonks.toFixed(2)} leva`);
}
cinema(["Premiere",
"10",
"12"])
